{
  "cli": {
    "version": ">= 13.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://nomoscribe-api-dev.vercel.app/",
        "EXPO_PUBLIC_CLOUDFRONT_DOMAIN": "dkt41sp0pphbb.cloudfront.net",
        "EXPO_PUBLIC_APP_SECRET_KEY": "@nomoscribe:app_secret"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://nomoscribe-api-dev.vercel.app/",
        "EXPO_PUBLIC_CLOUDFRONT_DOMAIN": "dkt41sp0pphbb.cloudfront.net",
        "EXPO_PUBLIC_APP_SECRET_KEY": "@nomoscribe:app_secret"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_API_BASE_URL": "https://nomoscribe-api.vercel.app/",
        "EXPO_PUBLIC_CLOUDFRONT_DOMAIN": "dkt41sp0pphbb.cloudfront.net",
        "EXPO_PUBLIC_APP_SECRET_KEY": "@nomoscribe:app_secret"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production-aab": {
      "extends": "production",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "local-production": {
      "extends": "production",
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://nomoscribe-api.vercel.app/",
        "EXPO_PUBLIC_CLOUDFRONT_DOMAIN": "dkt41sp0pphbb.cloudfront.net",
        "EXPO_PUBLIC_APP_SECRET_KEY": "@nomoscribe:app_secret"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "simulator": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}
